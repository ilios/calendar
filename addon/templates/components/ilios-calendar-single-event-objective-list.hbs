{{#if domains.length}}
  <ul class='tree'>
    {{#each domains as |domain|}}
      <li>
        {{domain.title}}
        <ul>
          {{#each domain.objectives as |title|}}
            <li>{{{title}}}</li>
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ul>
{{else}}
  <li class="no-content">{{noContentPhrase}}</li>
{{/if}}
